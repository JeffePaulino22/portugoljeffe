algoritmo"crescente"

var
 x , y : inteiro


inicio
escreval("DIGITE DOIS NUMEROS :")
leia(x)
leia(y)

enquanto x <> y faca

    se x < y entao
        escreval("CRESCENTE")
    senao
        escreval("DECRESCENTE")
    fimse
fimenquanto

escreval("DIGITE OUTROS DOIS NUMEROS: ")
leia(X)
leia(y)


fimalgoritmo